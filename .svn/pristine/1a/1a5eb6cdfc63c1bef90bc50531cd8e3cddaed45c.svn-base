* {
  margin: 0px;
  padding: 0px;
  /* 在移动端不允许出现横向滚动条的不能让padding或者border撑大盒子 */
  box-sizing: border-box;
  /*  清除移动端点击（按住）高亮颜色 */
  -webkit-tap-highlight-color: transparent;
}
input {
  outline: none;
  border: 0;
}
a:hover {
  text-decoration: none;
}
li {
  list-style: none;
  position: relative;
}
i,
em {
  font-style: normal;
}
b {
  font-weight: normal;
}
#header {
  height: 44px;
  background-color: #fe4a65;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 999;
}
#header .main-logo {
  position: absolute;
  left: 10px;
  height: 44px;
  line-height: 44px;
  color: #fff;
  font-weight: bold;
  font-size: 16px;
}
#header .search {
  height: 28px;
  line-height: 28px;
  margin-top: 7px;
  margin-right: 30px;
  margin-left: 64px;
}
#header .search > input {
  height: 26px;
  line-height: 26px;
  border-radius: 5px;
  font-size: 12px;
  padding-left: 10px;
  color: #666;
  width: 100%;
}
#header .main-cat {
  position: absolute;
  right: 10px;
  top: 0px;
  height: 44px;
  line-height: 44px;
}
#header .main-cat > i {
  color: #fff;
}
#main {
  margin-top: 44px;
  margin-bottom: 96px;
  height: 100%;
}
#main #slide {
  width: 100%;
}
#main #slide img {
  width: 100%;
}
#main #slide > .swiper-pagination > span {
  background-color: #fff;
}
#main #nav > .baicaijiaTitle {
  overflow: hidden;
  width: 100%;
}
#main #nav > .baicaijiaTitle > ul {
  border-bottom: 1px solid #ccc;
  height: 35px;
  line-height: 35px;
  width: 1386.66666667px;
  margin-bottom: 0;
}
#main #nav > .baicaijiaTitle > ul > li {
  width: 106.66666667px;
  text-align: center;
  border-right: 1px solid #ccc;
}
#main #nav > .baicaijiaTitle > ul > li:last-child {
  border-right: none;
}
#main #nav > .baicaijiaTitle > ul > li.active > a {
  color: red;
}
#main #nav > .baicaijiaTitle > ul > li > a {
  color: #666;
}
#main #product-list .product {
  background-color: #fff;
  margin-bottom: 10px;
}
#main #product-list .product > .container > .row {
  height: 150px;
}
#main #product-list .product > .container > .row > .col-xs-4 > a {
  height: 150px;
  display: block;
}
#main #product-list .product > .container > .row > .col-xs-4 > a > img {
  width: 100%;
}
#main #product-list .product > .container > .row > .col-xs-8 {
  padding-top: 5px;
  padding-bottom: 5px;
  height: 150px;
}
#main #product-list .product > .container > .row > .col-xs-8 > .b {
  font-size: 16px;
}
#main #product-list .product > .container > .row > .col-xs-8 > .price {
  margin: 15px 0;
  font-size: 20px;
  color: #f4483e;
}
#main #product-list .product > .container > .row > .col-xs-8 > .price > em {
  font-size: 12px;
}
#main #product-list .product > .container > .row > .col-xs-8 > .price > del {
  color: #ccc;
  font-size: 12px;
}
#main #product-list .product > .container > .row > .col-xs-8 > .bar {
  margin-bottom: 10px;
}
#main #product-list .product > .container > .row > .col-xs-8 > .bar > i {
  float: left;
  width: 100px;
  background-color: #ffb7b7;
  border-radius: 15px;
  height: 15px;
  font-size: 12px;
  text-align: center;
  color: #fff;
  border: 1px solid #f4483e;
}
#main #product-list .product > .container > .row > .col-xs-8 > .bar > b {
  float: left;
  margin-left: 10px;
  line-height: 14px;
  font-size: 14px;
  color: #f4483e;
}
#main #product-list .product > .container > .row > .col-xs-8 > .discount {
  width: 130px;
  height: 25px;
  border: 1px solid #f4483e;
  text-align: center;
  line-height: 25px;
}
#main #product-list .product > .container > .row > .col-xs-8 > .discount > a {
  font-size: 14px;
  color: #f4483e;
}
#main #product-list .product > .container > .row > .col-xs-8 > a {
  position: absolute;
  right: 15px;
  bottom: 15px;
  height: 25px;
  width: 55px;
  text-align: center;
  line-height: 25px;
  background-color: #f4483e;
  color: #fff;
  font-size: 12px;
  border-radius: 5px;
}
#footer {
  z-index: 999;
  position: relative;
  bottom: 0;
  left: 0;
  background-color: #efefef;
  color: #666;
  width: 100%;
}
#footer > .container > .row {
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
}
#footer > .container > .row > div {
  height: 36px;
  text-align: center;
  line-height: 36px;
  border-right: 1px solid #c4c4c4;
}
#footer > .website {
  font-size: 12px;
  text-align: center;
  line-height: 14px;
  padding: 15px 0;
  height: 60px;
}
#footer > .website > p > .phone {
  color: #666;
}
#footer > .website > p > .phone-app {
  color: red;
}
