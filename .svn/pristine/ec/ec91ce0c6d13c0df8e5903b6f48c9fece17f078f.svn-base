#main {
  padding: 10px;
}
#main > p {
  margin: 0;
}
#main > p > span {
  font-size: 10px;
  color: #666;
}
#main > p > span.author {
  color: #f18622;
}
#main .content {
  margin-top: 10px;
  width: 100%;
}
#main .content img {
  height: 120px;
  float: right;
}
#main .largeImage {
  width: 100%;
}
#main .product-city {
  margin-top: 10px;
}
#main .product-city #disstorck {
  padding-left: 100px;
}
#main .product-city #disstorck > li {
  list-style: none;
}
#main .product-city #disstorck > li > a {
  border: 1px solid #aaa;
  margin-top: -1px;
  width: 100px;
  display: block;
}
#main .product-city #disstorck > li > a > span {
  width: 100%;
}
#main .product-city #disstorck > li > a > span > strong {
  float: right;
  color: #f18622;
}
#main #buyProduct {
  margin-top: 5px;
  padding: 5px 20px 5px 20px;
}
#main #buyProduct button {
  width: 100%;
  height: 30px;
  background-color: #f18622;
  color: white;
  font-size: 20px;
  text-align: center;
  line-height: 30px;
  border: none;
  border-radius: 5px;
}
#main #qrCode {
  width: 100%;
  margin-top: 5px;
}
#main .comments {
  width: 100%;
  padding: 0 5px;
  margin-top: 5px;
}
#main .comments .cu-content-pl {
  width: 100%;
}
#main .comments .cu-content-pl .tit {
  font-size: 20px;
  padding: 5px;
}
#main .comments .cu-content-pl .reply {
  width: 100%;
}
#main .comments .cu-content-pl .reply .form {
  width: 100%;
}
#main .comments .cu-content-pl .reply .form > textarea {
  resize: none;
  width: 100%;
  height: 120px;
}
#main .comments .cu-content-pl .reply .form .ctrl > input {
  height: 50px;
  width: 100px;
  font-size: 20px;
  color: white;
  position: relative;
  background-color: #f18622;
  border: 0;
  bottom: 55px;
  right: -190px;
}
#main .comments .cu-content-pl #newcomment {
  margin-top: -45px;
}
#main .comments .cu-content-pl .list {
  position: relative;
}
#main .comments .cu-content-pl .list > ul > li {
  list-style: none;
}
#main .comments .cu-content-pl .list > ul > li .userimg > img {
  float: left;
  height: 40px;
}
#main .comments .cu-content-pl .list > ul > li .con .name {
  position: absolute;
  right: 0;
}
#main .comments .cu-content-pl .list > ul > li .con .name .username {
  float: left;
}
#main .comments .cu-content-pl .list > ul > li .con .name .time {
  float: right;
}
#main .comments .cu-content-pl .list > ul > li .con .content {
  padding: 40px;
}
#footer p {
  margin: 0;
