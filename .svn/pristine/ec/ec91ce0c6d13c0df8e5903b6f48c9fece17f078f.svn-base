#header {
  color: #fff;
  height: 50px;
}
#header .arrow-left {
  color: #fff;
  font-size: 20px;
  margin-top: 5px;
}
#header > p {
  text-align: center;
  font-size: 20px;
  margin: 0;
  margin-top: -30px;
