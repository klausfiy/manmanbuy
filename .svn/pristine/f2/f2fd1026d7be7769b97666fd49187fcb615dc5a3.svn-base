* {
  margin: 0px;
  padding: 0px;
  /* 在移动端不允许出现横向滚动条的不能让padding或者border撑大盒子 */
  box-sizing: border-box;
  /*  清除移动端点击（按住）高亮颜色 */
  -webkit-tap-highlight-color: transparent;
}
input {
  outline: none;
  border: 0;
}
a:hover {
  text-decoration: none;
}
li {
  list-style: none;
  position: relative;
}
i,
em {
  font-style: normal;
}
b {
  font-weight: normal;
}
#header {
  padding: 10px 10px 10px 20px;
  background-color: #f18622;
  width: 100%;
}
#header .logo-left {
  display: block;
  margin-top: 5px;
  height: 25px;
  float: left;
}
#header .logo-left > img {
  height: 100%;
}
#header .onload-app-right {
  display: block;
  height: 35px;
  float: right;
}
#header .onload-app-right > img {
  height: 100%;
}
#search .search {
  padding: 10px;
  border-bottom: 1px solid #ccc;
  width: 100%;
  overflow: hidden;
  position: relative;
}
#search .search > form {
  margin-right: 70px;
}
#search .search > form > input {
  background-color: #eee;
  height: 30px;
  width: 100%;
  border-radius: 15px;
  border: none;
  text-indent: 20px;
}
#search .search > button {
  background-color: #f18622;
  height: 30px;
  width: 60px;
  color: #fff;
  border: none;
  border-radius: 15px;
  font-size: 16px;
  position: absolute;
  top: 10px;
  right: 10px;
}
#main {
  background-color: #f2f2f2;
}
#main #nav {
  height: 36px;
  line-height: 36px;
  padding-left: 15px;
  border-bottom: 1px solid #ccc;
  background-color: #eeeeee;
}
#main #nav > a {
  color: #666666;
}
#main #brandsList {
  margin-top: 10px;
  margin-bottom: 10px;
  background-color: #eeeeee;
  padding: 0 5px;
}
#main #brandsList > .brandsList-title {
  height: 36px;
  color: #ba9978;
  padding-left: 15px;
  line-height: 36px;
  font-size: 16px;
  border: 1px solid #ccc;
  margin-bottom: 10px;
}
#main #brandsList > .brandsList-brands > ul > li {
  position: relative;
  color: #000;
  padding: 10px;
  font-size: 16px;
  border-bottom: 1px solid #ccc;
}
#main #brandsList > .brandsList-brands > ul > li > a {
  display: block;
  width: 100%;
  color: #000;
}
#main #brandsList > .brandsList-brands > ul > li > a > span {
  position: absolute;
  top: 8px;
  width: 20px;
  height: 20px;
  background-color: #c9c9c9;
  text-align: center;
  color: #fff;
  font-size: 14px;
}
#main #brandsList > .brandsList-brands > ul > li > a > span.top1 {
  background-color: #f10e0e;
}
#main #brandsList > .brandsList-brands > ul > li > a > span.top2 {
  background-color: #ff9314;
}
#main #brandsList > .brandsList-brands > ul > li > a > span.top3 {
  background-color: #8adf5b;
}
#main #brandsList > .brandsList-brands > ul > li > a > .inner {
  margin-left: 30px;
  height: 40px;
  line-height: 40px;
  color: #999;
  font-size: 12px;
}
#main #brandsList > .brandsList-brands > ul > li > a > .inner > p {
  line-height: 16px;
  margin: 0;
  font-size: 13px;
}
#main #brandsList > .brandsList-brands > ul > li > a > .inner > p.title {
  font-size: 16px;
  color: #000;
  margin-bottom: 5px;
}
#main #brandsList > .brandsList-brands > ul > li > a > i {
  position: absolute;
  right: 10px;
  top: 10px;
  margin-top: 10px;
  color: #c8c8c8;
}
#footer {
  z-index: 999;
  position: relative;
  bottom: 0;
  left: 0;
  background-color: #efefef;
  color: #666;
  width: 100%;
}
#footer > .container > .row {
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
}
#footer > .container > .row > div {
  height: 36px;
  text-align: center;
  line-height: 36px;
  border-right: 1px solid #c4c4c4;
}
#footer > .website {
  font-size: 12px;
  text-align: center;
  line-height: 14px;
  padding: 15px 0;
  height: 60px;
}
#footer > .website > p > .phone {
  color: #666;
}
#footer > .website > p > .phone-app {
  color: red;
}
